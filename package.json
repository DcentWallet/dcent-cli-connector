{
  "name": "dcent-cli-connector",
  "version": "0.1.1-beta",
  "description": "D'CENT CLI SDK Connector",
  "main": "dist/index.js",
  "scripts": {
    "dev": "npm run build-dev",
    "test": "jest --detectOpenHandles --coverage --forceExit --runInBand",
    "unit-mock": "jest --coverage --runInBand 0_mock_test",
    "build": "webpack --config webpack.config.js --mode production",
    "build-dev": "webpack --config webpack.config.js --mode development",
    "lint": "eslint --ext .js src ",
    "lint:fix": "eslint --ext .js src --fix"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "testSequencer": "./tests/testSequencer.js",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "coverageDirectory": "tests/coverage",
    "collectCoverage": true,
    "setupFilesAfterEnv": [
      "./tests/jest.setup.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DcentWallet/dcent-cli-connector.git"
  },
  "keywords": [
    "D'CENT",
    "dcent",
    "hardware wallet",
    "wallet sdk",
    "D'CENT biometric wallet",
    "dcent sdk",
    "dcent cli sdk",
    "dcentwallet",
    "dcent cli connector"
  ],
  "homepage": "https://github.com/DcentWallet/dcent-cli-connector#readme",
  "author": "IoTrust Co. Ltd.,",
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "babel-jest": "^28.1.0",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "jest": "^28.1.0",
    "jest-environment-jsdom": "^28.1.0",
    "path": "^0.12.7",
    "regenerator-runtime": "^0.13.3",
    "webpack": "^4.40.2",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.11.2"
  },
  "dependencies": {
    "bignumber.js": "^9.0.0",
    "ethereumjs-abi": "^0.6.8",
    "ethereumjs-util": "^7.1.4",
    "google-protobuf": "^3.9.0",
    "node-hid": "^2.1.1",
    "ripple-binary-codec": "^1.0.2",
    "rlp": "^3.0.0",
    "usb-detection": "^4.14.1"
  },
  "engines": {
    "node": ">= 14.0.0",
    "npm": ">= 6.0.0"
  },
  "license": "MIT",
  "files": [
    "dist"
  ]
}
